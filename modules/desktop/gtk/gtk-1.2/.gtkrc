# GTK+ 1.2 configuration - Nostalgia Windows 95
# Path: ~/.gtkrc

style "default"
{
  fontset = "-*-dejavu sans-bold-r-normal-*-11-*-*-*-*-*-iso8859-1"
  
  bg[NORMAL]      = "#d4d0c8"
  bg[PRELIGHT]    = "#eaeaea"
  bg[ACTIVE]      = "#808080"
  bg[SELECTED]    = "#0a246a"
  bg[INSENSITIVE] = "#d4d0c8"

  fg[NORMAL]      = "#000000"
  fg[PRELIGHT]    = "#000000"
  fg[ACTIVE]      = "#000000"
  fg[SELECTED]    = "#ffffff"
  fg[INSENSITIVE] = "#808080"
  
  base[NORMAL]    = "#ffffff"
  base[SELECTED]  = "#0a246a"
  
  text[NORMAL]    = "#000000"
  text[SELECTED]  = "#ffffff"
}

class "GtkWidget" style "default"
